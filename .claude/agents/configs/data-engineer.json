{
  "agentName": "data-engineer",
  "description": "Use PROACTIVELY for ETL/ELT pipelines, data transformations, and batch processing. MUST BE USED with Supabase MCP for data migration operations, pipeline design, and data quality validation.",
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase",
        "--project-ref=${SUPABASE_PROJECT_REF}"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      }
    }
  },
  "capabilities": [
    "Data pipeline design",
    "ETL operations",
    "Data transformation scripts",
    "Batch processing",
    "Data quality validation",
    "Data migration"
  ],
  "response_format": {
    "type": "pipeline_summary",
    "max_tokens": 600,
    "include": [
      "pipeline_design",
      "data_flow",
      "transformation_logic",
      "validation_rules"
    ],
    "exclude": [
      "full_dataset_dumps",
      "verbose_processing_logs"
    ]
  },
  "routing_triggers": [
    "data pipeline",
    "ETL",
    "data transformation",
    "batch process",
    "data migration",
    "data quality"
  ],
  "special_instructions": [
    "Design scalable data pipelines",
    "Use Supabase RPC functions for complex transforms",
    "Include data validation and error handling",
    "Return pipeline code, not processed data"
  ]
}
